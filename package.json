{
  "name": "PhotoDisplay",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "frontend/",
    "backend/"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "deps": "turbo run deps",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,html,css,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.25.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19",
  "lint-staged": {
    "**/*.{md}": []
  }
}
